{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build and Run macOS App",
			"type": "shell",
			"command": "xcodebuild",
			"args": [
				"-project",
				"sdvg.xcodeproj",
				"-scheme",
				"sdvg",
				"-configuration",
				"Debug",
				"build"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}